{% extends 'base.html.twig' %}

{% block title %}Conteneurs{% endblock %}

{% block body %}

    <h1>Liste des conteneurs</h1>

    {% for container in listContainer %}
        <p>{{ container.id }} {{ container.containerModel.name }} {{ container.containership.name }} <a href="container/{{ container.id }}">Voir</a></p>
    {% endfor %}
{% endblock %}